type Result = 
 variant {
   err: text;
   ok: Post;
 };
type Post = 
 record {
   author: opt text;
   content: text;
   id: nat;
   timestamp: int;
   title: text;
 };
service : {
  createPost: (text, text, opt text) -> (Result);
  getPostById: (nat) -> (opt Post) query;
  getPosts: () -> (vec Post) query;
}
